{% extends 'base-backend.html.twig' %}

{% block title %}Editeur{% endblock %}

{% block body %}
<a href="{{ path('app_admin_editeur_index') }}" class="btn btn-dark">Retour Ã  la liste</a>
    <h1>Editeur</h1>
        <div class="d-flex justify-content-end align-items-end">
        {{ include('admin_editeur/_delete_form.html.twig') }}
        <a href="{{ path('app_admin_editeur_edit', {'id': editeur.id}) }}" class="btn btn-success ms-2"><i class="align-middle" data-feather="edit-2"></i></a>
        </div> 
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ editeur.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ editeur.nom }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ editeur.description }}</td>
            </tr>
            <tr>
                <th>LogoName</th>
                <td>{{ editeur.logoName }}</td>
            </tr>
            <tr>
                <th>ImageName</th>
                <td>{{ editeur.imageName }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ editeur.updatedAt ? editeur.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    

{% endblock %}
